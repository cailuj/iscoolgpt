{
  "family": "iscoolgpt-task",
  "executionRoleArn": "arn:aws:iam::331374384030:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "containerDefinitions": [
    {
      "name": "iscoolgpt",
      "image": "331374384030.dkr.ecr.us-east-1.amazonaws.com/iscoolgpt-backend:latest",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}