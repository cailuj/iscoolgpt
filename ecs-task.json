{
  "family": "iscoolgpt-task",
  "executionRoleArn": "arn:aws:iam::331374384030:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::331374384030:role/ecsTaskExecutionRole",
  
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  
  "cpu": "1024",
  "memory": "3072",

  "containerDefinitions": [
    {
      "name": "iscoolgpt",

      "essential": true,

      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],

      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/iscoolgpt",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}